---
import ListCard from './ListCard.astro';

const { listings } = Astro.props;
---

<section class="bg-grey-100 w-full mx-auto">
  <div class="grid xl:grid-cols-2 lg:grid-cols-1 p-10 gap-10">
    {
      listings.map((listing: any) => {
        return <ListCard listing={listing} />;
      })
    }
  </div>
</section>
